{
  "name": "markdown-it-marked",
  "version": "0.2.2",
  "description": "Plugin for markdown-it for marking substrings within text",
  "license": "Apache-2.0",
  "main": "dist/markdown-it-marked.js",
  "module": "src/index.js",
  "exports": {
    "require": "./dist/markdown-it-marked.js",
    "default": "./src/index.js"
  },
  "scripts": {
    "lint": "eslint .",
    "prepare": "rollup --config",
    "pretest": "eslint .",
    "test": "jest --testRegex test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VisualMeaning/markdown-it-marked.git"
  },
  "keywords": [
    "markdown-it-plugin",
    "markdown-it",
    "markdown",
    "marked",
    "highlight",
    "mark"
  ],
  "author": "Martin Packman <martin.packman@visual-meaning.com>",
  "bugs": {
    "url": "https://github.com/VisualMeaning/markdown-it-marked/issues"
  },
  "homepage": "https://github.com/VisualMeaning/markdown-it-marked#readme",
  "sideEffects": false,
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.12.0",
    "eslint-plugin-jest": "^24.1.0",
    "jest": "^26.6.1",
    "markdown-it": "^12.0.2",
    "rollup": "^2.32.1",
    "rollup-plugin-terser": "^7.0.2"
  }
